#!/usr/bin/make -f

export DH_GOLANG_INSTALL_EXTRA := \
	typeparams/go.mod

export DH_GOLANG_EXCLUDES := \
	cmd \
	errors \
	event \
	example \
	jsonrpc2 \
	shiny \
	shootout \
	slog/benchmarks/zap_benchmarks \
	slog/benchmarks/zerolog_benchmarks \
	sumdb

%:
	dh $@ --builddirectory=_build --buildsystem=golang
